  },
  "devDependencies": {
    "@types/node": "^18",
    "gh-pages": "^5.0.0",
    "nuxt": "^3.4.3",
    "sass": "^1.62.1",
    "typescript": "^5.0.4",
    "vue-tsc": "^1.6.4"
  },
  "overrides": {
    "vue": "latest"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.4.11",
    "http-server": "^14.1.1",
    "normalize.css": "^8.0.1",
    "pinia": "^2.1.1"
  }
}
